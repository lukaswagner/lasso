    "@types/gl-matrix": "^3.2.0",
    "concurrently": "^7.0.0",
    "css-loader": "^6.6.0",
    "gl-matrix": "^3.4.3",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.5.3",
